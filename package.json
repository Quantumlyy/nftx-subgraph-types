  "devDependencies": {
    "@graphql-codegen/c-sharp": "3.0.2",
    "@graphql-codegen/c-sharp-operations": "1.19.5",
    "@graphql-codegen/cli": "1.21.8",
    "@graphql-codegen/introspection": "1.18.2",
